{
	"format_version": "1.16.0",
	"minecraft:entity": {
		"description": {
			"identifier": "polar:killaura",
			"is_spawnable": false,
			"is_summonable": true,
			"is_experimental": false,
			"scripts": {
				"animate": [
					"bridge_execute_commands"
				]
			},
			"animations": {
				"bridge_execute_commands": "controller.animation.bridge_killaura.execute_commands"
			}
		},
		"component_groups": {
			"despawn": {
				"minecraft:instant_despawn": {
					"remove_child_entities": false
				}
			},
			"bridge:execute_command_id_1": {
				"minecraft:skin_id": {
					"value": 1
				}
			},
			"bridge:execute_no_command": {
				"minecraft:skin_id": {
					"value": 0
				}
			}
		},
		"components": {
			"minecraft:collision_box": {
				"width": 0.6,
				"height": 1.8
			},
			"minecraft:physics": {
				"has_gravity": false
			},
			"minecraft:health": {
				"value": 1,
				"max": 1
			},
			"minecraft:type_family": {
				"family": [
					"killaura",
					"player"
				]
			},
			"minecraft:damage_sensor": {
				"triggers": [
					{
						"deals_damage": true,
						"on_damage": {
							"target": "self",
							"event": "failed",
							"filters": {
								"test": "has_damage",
								"subject": "self",
								"value": "entity_attack"
							}
						},
						"cause": "entity_attack"
					}
				]
			},
			"minecraft:timer": {
				"looping": false,
				"time_down_event": {
					"event": "timer up"
				},
				"time": [
					0.9,
					0.9
				]
			}
		},
		"events": {
			"failed": {
				"sequence": [
					{
						"add": {
							"component_groups": [
								"bridge:execute_command_id_1"
							]
						}
					},
					{
						"add": {
							"component_groups": [
								"despawn"
							]
						}
					}
				]
			},
			"timer up": {
				"add": {
					"component_groups": [
						"despawn"
					]
				}
			},
			"bridge:remove_command_id_1": {
				"add": {
					"component_groups": [
						"bridge:execute_no_command"
					]
				},
				"remove": {
					"component_groups": [
						"bridge:execute_command_id_1"
					]
				}
			}
		}
	}
}